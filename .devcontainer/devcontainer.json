{
	"name": "data-structures-and-algorithms",
	"dockerComposeFile": [
		"docker-compose.yaml"
	],
	"service": "devcontainer",
	"workspaceFolder": "/workspaces/data-structures-and-algorithms",
	"postCreateCommand": "cd $WORKSPACE && poetry install",
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.profiles.linux": {
					"bash": {
						"path": "bash",
						"icon": "terminal-bash"
					}
				}
			},
			"extensions": [
				"ms-azuretools.vscode-docker",
				"shd101wyy.markdown-preview-enhanced",
				"github.vscode-github-actions",
				"ms-python.python",
				"ms-python.vscode-pylance",
				"charliermarsh.ruff",
				"ms-python.mypy-type-checker"
			]
		}
	}
}